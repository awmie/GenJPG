{
	"name": "image-generator",
	"version": "1.0.0",
	"description": "Image generation frontend",
	"scripts": {
		"build": "esbuild src/app.tsx --bundle --outfile=static/js/bundle.js --format=esm --platform=browser --target=es2020 --jsx-factory=h --jsx-fragment=Fragment",
		"watch": "esbuild src/app.tsx --bundle --outfile=static/js/bundle.js --format=esm --platform=browser --target=es2020 --jsx-factory=h --jsx-fragment=Fragment --watch"
	},
	"devDependencies": {
		"typescript": "^4.9.5",
		"@types/node": "^20.11.0",
		"esbuild": "^0.19.11"
	},
	"dependencies": {
		"preact": "^10.19.3"
	}
}
